{
   "name": "@neuekit/utils",
   "version": "2.0.0",
   "license": "MIT",
   "description": "Tree-shakeable JavaScript utilities, add yours today!",
   "author": "Matt Pilott && Darby Manning",
   "homepage": "https://github.com/neuekit/utils#readme",
   "repository": {
      "type": "git",
      "url": "git+https://github.com/neuekit/utils.git"
   },
   "bugs": {
      "url": "https://github.com/neuekit/utils/issues"
   },
   "publishConfig": {
      "access": "public"
   },
   "module": "src/index.js",
   "type": "module",
   "exports": {
      ".": {
         "default": "./src/index.js"
      },
      "./integrations": {
         "default": "./src/integrations/index.js"
      },
      "./middleware": {
         "default": "./src/middleware/index.js"
      },
      "./svelte": {
         "default": "./src/svelte/index.js"
      },
      "./vanilla": {
         "default": "./src/vanilla/index.js"
      }
   },
   "scripts": {
      "build": "rollup --config",
      "release": "release-it",
      "test": "uvu src test.js -i _template",
      "documentation": "documentation build ./src/** -f md -o docs/index.md",
      "prettier": "prettier --write --plugin-search-dir=. .",
      "watch:rollup": "rollup --config --watch",
      "watch:documentation": "onchange './src/**/*.js' -- npm run documentation",
      "watch:prettier": "onchange 'rollup.config.js' 'src/**/*.js' -- prettier --write --plugin-search-dir=. {{changed}}",
      "watch": "run-p -s 'watch:prettier' 'watch:documentation'",
      "dev": "run-p -s 'watch:rollup' 'watch:prettier'"
   },
   "devDependencies": {
      "@babel/core": "^7.18.9",
      "@babel/plugin-transform-runtime": "^7.18.9",
      "@babel/preset-env": "^7.18.9",
      "@babel/runtime": "^7.18.9",
      "@rollup/plugin-babel": "^5.3.1",
      "@rollup/plugin-commonjs": "^22.0.1",
      "@rollup/plugin-node-resolve": "^13.3.0",
      "@rollup/plugin-replace": "^4.0.0",
      "benchmark": "^2.1.4",
      "documentation": "^13.2.5",
      "dotenv": "^16.0.1",
      "esm": "^3.2.25",
      "global-jsdom": "^8.5.0",
      "jsdom": "^20.0.0",
      "npm-run-all": "^4.1.5",
      "onchange": "^7.1.0",
      "prettier": "^2.7.1",
      "release-it": "^15.1.2",
      "rollup": "^2.77.0",
      "rollup-plugin-terser": "^7.0.2",
      "tinydate": "^1.3.0",
      "uvu": "^0.5.6"
   }
}
